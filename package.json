{
  "name": "aims-wa",
  "version": "0.1.0",
  "description": "# Web Application",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll",
    "build": "ng build",
    "generate": "graphql-codegen",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html}\" --quiet --fix",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" --write",
    "commit": "cz",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/sarodriguezva/aims_web_app#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.5",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@apollo/client": "^3.0.0",
    "apollo-angular": "^4.1.0",
    "bootstrap": "^5.2.2",
    "graphql": "^16.6.0",
    "jquery": "^3.6.1",
    "module-alias": "^2.2.2",
    "popper.js": "^1.16.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "^14.2.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/typescript": "^2.7.5",
    "@graphql-codegen/typescript-apollo-angular": "3.5.5",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "commitizen": "^4.2.5",
    "cz-git": "^1.3.11",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "typescript": "~4.7.2",
    "@graphql-codegen/introspection": "2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "main": "commitlint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarodriguezva/aims_wa.git"
  },
  "author": "AIMS",
  "bugs": {
    "url": "https://github.com/sarodriguezva/aims_wa/issues"
  }
}